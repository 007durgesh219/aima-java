<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="266.0" prefWidth="460.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="aima.gui.demo.search.tree.info.FrontierInfoController">
   <center>
      <BorderPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <center>
            <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <GridPane>
                    <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <Label text="State" />
                        <Label text="Parent" GridPane.rowIndex="1" />
                        <Label text="Action" GridPane.rowIndex="2" />
                        <Label text="Path Cost g(n)" GridPane.rowIndex="3" />
                        <Label fx:id="stateLabel" text="-" GridPane.columnIndex="1" />
                        <Label fx:id="parentLabel" text="-" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                        <Label fx:id="actionLabel" text="-" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                        <Label fx:id="pathCostLabel" text="0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                     </children>
                  </GridPane>
               </children>
            </AnchorPane>
         </center>
         <top>
            <Label text="Next Node to Expand" underline="true" BorderPane.alignment="CENTER_LEFT" />
         </top>
      </BorderPane>
   </center>
   <padding>
      <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
   </padding>
   <top>
      <BorderPane prefHeight="61.0" prefWidth="456.0" BorderPane.alignment="CENTER">
         <top>
            <Label text="Frontier" underline="true" BorderPane.alignment="TOP_LEFT" />
         </top>
         <bottom>
            <HBox prefHeight="38.0" prefWidth="460.0" spacing="3.0" BorderPane.alignment="CENTER">
               <children>
                  <BorderPane prefHeight="200.0" prefWidth="200.0">
                     <top>
                        <ProgressBar fx:id="frontierProgress" prefWidth="200.0" progress="0.0" BorderPane.alignment="CENTER" />
                     </top>
                     <center>
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="3.0" BorderPane.alignment="CENTER">
                           <children>
                              <Label text="#current" />
                              <Label fx:id="currentFrontierCountLabel" text="0" />
                           </children>
                        </HBox>
                     </center>
                  </BorderPane>
                  <Label text="#max" />
                  <Label fx:id="maxFrontierCountLabel" text="0" />
               </children>
            </HBox>
         </bottom>
         <padding>
            <Insets bottom="2.0" />
         </padding>
      </BorderPane>
   </top>
</BorderPane>
